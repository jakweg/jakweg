{
	"name": "portfolio",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "npm-run-all --parallel images vite typesafe-i18n",
		"vite": "vite",
		"typesafe-i18n": "typesafe-i18n",
		"build": "npm run images && vite build && workbox generateSW workbox-config.json",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"images": "node resize-images.js"
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "latest",
		"@sveltejs/kit": "latest",
		"@typescript-eslint/eslint-plugin": "latest",
		"@typescript-eslint/parser": "latest",
		"eslint": "latest",
		"eslint-config-prettier": "latest",
		"eslint-plugin-svelte3": "latest",
		"prettier": "latest",
		"prettier-plugin-svelte": "latest",
		"svelte": "latest",
		"svelte-check": "latest",
		"tslib": "latest",
		"typesafe-i18n": "latest",
		"typescript": "latest",
		"vite": "latest",
		"workbox-cli": "latest",
		"npm-run-all": "latest"
	},
	"type": "module"
}
